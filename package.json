{
  "name": "younique-wedding-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "analyze": "npm run build && source-map-explorer 'build/static/js/*.js'",
    "lint": "eslint 'src/**/*.{ts,tsx,js,jsx}' --fix",
    "start": "react-scripts start",
    "build": "CI=false react-scripts build",
    "test": "npm run test:e2e:cli:server && npm run test:unit:coverage",
    "test:unit": "react-scripts test --env=jest-environment-jsdom-sixteen",
    "test:unit:coverage": "react-scripts test --env=jest-environment-jsdom-sixteen --watchAll=false --coverage",
    "test:e2e:cli": "cypress run",
    "test:e2e:cli:server": "start-server-and-test start :3000 test:e2e:cli",
    "test:e2e:gui": "cypress open",
    "test:e2e:gui:server": "start-server-and-test start :3000 test:e2e:gui"
  },
  "dependencies": {
    "@material-ui/core": "4.11.0",
    "@material-ui/icons": "4.9.1",
    "@sentry/browser": "5.21.3",
    "@testing-library/jest-dom": "5.9.0",
    "@testing-library/react": "10.4.9",
    "@testing-library/user-event": "10.4.0",
    "@types/jest": "26.0.10",
    "@types/node": "12.12.58",
    "@types/react": "16.9.46",
    "@types/react-dom": "16.9.8",
    "@types/react-router-dom": "5.1.5",
    "aws-amplify": "2.3.0",
    "clsx": "1.1.1",
    "i18next": "19.7.0",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-i18next": "11.7.1",
    "react-router-dom": "5.2.0",
    "react-scripts": "3.4.3",
    "typeface-roboto": "0.0.75",
    "typescript": "3.9.7"
  },
  "devDependencies": {
    "cypress": "4.12.1",
    "eslint-plugin-cypress": "2.11.1",
    "husky": "4.2.5",
    "jest-environment-jsdom-sixteen": "1.0.3",
    "source-map-explorer": "2.5.0",
    "start-server-and-test": "1.11.3"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "plugin:cypress/recommended"
    ],
    "rules": {
      "quotes": [
        2,
        "double"
      ],
      "semi": "error"
    }
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}",
      "!src/**/index.ts",
      "!src/**/*.d.ts"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
